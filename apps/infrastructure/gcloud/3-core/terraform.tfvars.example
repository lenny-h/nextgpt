# GCP configuration
google_vertex_project  = "your-gcp-project-id"
google_vertex_location = "us-central1"

# Site Configuration
site_url = "example.com"

# Authentication
better_auth_secret               = "your-secure-secret-key"
only_allow_admin_to_create_buckets = false
admin_user_ids                   = "" # Comma-separated list of admin user IDs
enable_email_signup              = true
allowed_email_domains            = "" # Comma-separated list of allowed email domains, leave empty to allow all domains

# OAuth Providers
enable_oauth_login   = true
google_client_id     = ""
google_client_secret = ""
github_client_id     = ""
github_client_secret = ""
gitlab_client_id     = ""
gitlab_client_secret = ""

# SSO Configuration
enable_sso                 = false
sso_domain                 = ""
sso_provider_id            = ""
sso_client_id              = ""
sso_client_secret          = ""
sso_issuer                 = ""
sso_authorization_endpoint = ""
sso_discovery_endpoint     = ""
sso_token_endpoint         = ""
sso_jwks_endpoint          = ""

# Email Configuration
resend_sender_email = "noreply@example.com"
resend_api_key      = "your-resend-api-key"

# Application Security
encryption_key = "your-64-character-encryption-key-here-replace-with-actual"

# Storage Configuration (Cloudflare R2)
use_cloudflare_r2            = false
cloudflare_access_key_id     = ""
cloudflare_secret_access_key = ""
r2_endpoint                  = "https://account-id.r2.cloudflarestorage.com"

# AI Models
embeddings_model = "text-embedding-004"
llm_models       = "gemini-2.5-flash,gemini-2.5-pro"