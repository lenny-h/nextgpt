# General Configuration
site_url = "nextgpt.cloud"
encryption_key = "your-encryption-key" # (can eg be generated by running 'openssl rand -hex 32' in the terminal)

# GCP Configuration
project_id = "your-gcp-project-id"
region     = "us-central1"
zone       = "us-central1-a"

# Database Configuration
db_password     = "your-super-secret-and-long-postgres-password"

# Supabase JWT Configuration
jwt_exp  = "3600"
# JWT_SECRET, ANON_KEY, and SERVICE_ROLE_KEY can be generated on https://supabase.com/docs/guides/self-hosting/docker#generate-api-keys
jwt_secret = "your-jwt-secret"
anon_key = "your-anon-key"
service_role_key = "your-service-role-key"
logflare_public_access_token = "your-logflare-public-access-token"  # (can eg be generated by running 'openssl rand -hex 32' in the terminal)
logflare_private_access_token = "your-logflare-private-access-token" # (can eg be generated by running 'openssl rand -hex 32' in the terminal)
studio_password = "this-password-is-insecure-and-should-be-updated"

# Cloudflare Configuration
cloudflare_api_token            = "your-40-token-cloudflare-api-token-abcde"
cloudflare_account_id           = "your-cloudflare-account-id"
r2_location                     = "your-r2-location" # (one of "apac", "eeur", "enam", "weur", "wnam", "oc"; apac -> Asia-Pacific, eeur -> Eastern Europe, enam -> Eastern North America, weur -> Western Europe, wnam -> Western North America, oc -> Oceania)
# To obtain the following two env variables, create an R2 api key with permission "Objects Read/Write"
cloudflare_r2_access_key_id     = "your-cloudflare-r2-access-key-id"
cloudflare_r2_secret_access_key = "your-cloudflare-r2-secret-access-key"
