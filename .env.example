# General Settings
GOOGLE_VERTEX_PROJECT = "your-gcp-project-id"
GOOGLE_VERTEX_LOCATION = "your-gcp-region" # (eg europe-west1)
WEB_URL = "http://localhost:3000" 
DASHBOARD_URL = "http://localhost:3001"
ENCRYPTION_KEY = "your-64-character-encryption-key" # (can eg be generated by running 'openssl rand -hex 32' in the terminal)

# Better Auth
BETTER_AUTH_URL = "http://localhost:3000" # URL of your frontend where Better Auth will redirect after login
BETTER_AUTH_SECRET = "your-better-auth-secret" # (can eg be generated by running 'openssl rand -hex 32' in the terminal)

# Google OAuth
GOOGLE_CLIENT_ID = "your-google-oauth-client-id"
GOOGLE_CLIENT_SECRET = "your-google-oauth-client-secret"

# Database Settings
DATABASE_URL = "postgresql://postgres:admin@localhost/mydb" # Has format: postgresql://USER:PASSWORD@HOST:PORT/DATABASE
REDIS_URL = "redis://localhost:6379" # Has format: redis://HOST:PORT

# Cloudflare Settings
CLOUDFLARE_API_TOKEN = "your-cloudflare-api-token"
CLOUDFLARE_ACCOUNT_ID = "your-cloudflare-account-id"

# R2_LOCATION must be one of "apac", "eeur", "enam", "weur", "wnam", "oc"
# apac -> Asia-Pacific, eeur -> Eastern Europe, enam -> Eastern North America
# weur -> Western Europe, wnam -> Western North America, oc -> Oceania
R2_LOCATION = "your-cloudflare-r2-bucket-location"
CLOUDFLARE_ACCESS_KEY_ID = "your-cloudflare-r2-access-key-id"
CLOUDFLARE_SECRET_ACCESS_KEY = "your-cloudflare-r2-secret-access-key"

# Google Cloud Settings
CLOUD_TASKS_SA = "cloud-tasks-sa@<your-gcp-project-id>.iam.gserviceaccount.com"
TASK_QUEUE_PATH = "projects/<your-gcp-project-id>/locations/<your-gcp-region>/queues/document-processing-queue"

# Model Settings
EMBEDDINGS_MODEL = "text-embedding-004"

ATTACHMENT_URL_PREFIX = "https://storage.googleapis.com/<your-gcp-project-id>-files-bucket/"