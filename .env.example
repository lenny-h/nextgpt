# General Settings
PROJECT_ID = "your-gcp-project-id"
REGION = "your-gcp-region" # (eg europe-west1)
SITE_URL = "http://localhost:3000" 
DASHBOARD_URL = "http://localhost:3001"
ENCRYPTION_KEY = "your-64-character-encryption-key" # (can eg be generated by running 'openssl rand -hex 32' in the terminal)

# Auth Settings
DISABLE_SIGNUP = "false"
ENABLE_EMAIL_SIGNUP = "true"
ENABLE_ANONYMOUS_USERS = "false"
ENABLE_EMAIL_AUTOCONFIRM = "false"

SMTP_ADMIN_EMAIL = "your-smtp-admin-email"
SMTP_HOST = "your-smtp-host" # (eg smtp.sendgrid.net)
SMTP_PORT = "your-smtp-port" # (usually 587)
SMTP_USER = "apikey"
SMTP_PASS = "your-smtp-password" # (eg SendGrid API key)
SMTP_SENDER_NAME = "your-smtp-sender-name" # (eg Name of your company)

MAILER_URLPATHS_INVITE = "/auth/v1/verify"
MAILER_URLPATHS_CONFIRMATION = "/auth/v1/verify"
MAILER_URLPATHS_RECOVERY = "/auth/v1/verify"
MAILER_URLPATHS_EMAIL_CHANGE = "/auth/v1/verify"

ENABLE_PHONE_SIGNUP = "false"
ENABLE_PHONE_AUTOCONFIRM = "false"

# Cloudflare Settings
CLOUDFLARE_API_TOKEN = "your-cloudflare-api-token"
CLOUDFLARE_ACCOUNT_ID = "your-cloudflare-account-id"
R2_LOCATION = "your-cloudflare-r2-bucket-location"
CLOUDFLARE_R2_ACCESS_KEY_ID = "your-cloudflare-r2-access-key-id"
CLOUDFLARE_R2_SECRET_ACCESS_KEY = "your-cloudflare-r2-secret-access-key"

# Google Cloud Settings
CLOUD_TASKS_SA = "email-of-service-account-for-cloud-tasks"

# Model Settings
EMBEDDINGS_MODEL = "text-embedding-004"

# Supabase Settings
DB_PASSWORD = "your-super-secret-and-long-postgres-password"
JWT_EXP = "3600"
# JWT_SECRET, ANON_KEY, and SERVICE_ROLE_KEY can be generated on https://supabase.com/docs/guides/self-hosting/docker#generate-api-keys
JWT_SECRET = "your-super-secret-jwt-token-with-at-least-32-characters"
ANON_KEY = "your-anon-key"
SERVICE_ROLE_KEY = "your-service-role-key"
LOGFLARE_PUBLIC_ACCESS_TOKEN = "your-super-secret-and-long-logflare-key-public" # (can eg be generated by running 'openssl rand -hex 32' in the terminal)
LOGFLARE_PRIVATE_ACCESS_TOKEN = "your-super-secret-and-long-logflare-key-private" # (can eg be generated by running 'openssl rand -hex 32' in the terminal)
STUDIO_PASSWORD = "this-password-is-insecure-and-should-be-updated"